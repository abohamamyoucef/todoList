
<%- include('header'); -%>
   <div class="box" id="heading">
      <h1><%= listTitel %></h1>
    </div>
    <div class="box">
      <% for (let i = 0; i < newlistite.length; i++) { %>
      <div class="item">
        <input type="checkbox" />
        <p><%= newlistite[i] %></p>
      </div>
      <% } %>
      <form class="item" action="/" method="post">
        <input
          type="text"
          name="newitem"
          placeholder="New item"
          autocomplete="off"
        />
        <button type="submit" name="list" value= <%= listTitel %> >+</button>
      </form>
    </div>
    <%- include('footer'); -%>